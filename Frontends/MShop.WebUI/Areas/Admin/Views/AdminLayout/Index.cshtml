@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("AdminLayoutHeadComponentPartial")
<body class="with-welcome-text">
    <div class="container-scroller">
        @await Component.InvokeAsync("AdminLayoutNavbarComponentPartial") 
        <div class="container-fluid page-body-wrapper">
            @await Component.InvokeAsync("AdminLayoutSidebarComponentPartial")
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="home-tab">
                                <div class="card">
                                    <div class="container">
                                        @RenderBody()
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @await Component.InvokeAsync("AdminLayoutFooterComponentPartial")
            </div>
        </div>
    </div>
    @await Component.InvokeAsync("AdminLayoutScriptsComponentPartial")
</body>
</html>
